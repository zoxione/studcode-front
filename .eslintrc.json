{
  "plugins": ["import"],
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
  "rules": {
    "import/order": [
      "warn",
      {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
        "newlines-between": "always"
      }
    ]
  }
}
